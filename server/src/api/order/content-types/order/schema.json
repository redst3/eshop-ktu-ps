{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "userId": {
      "type": "string",
      "required": true
    },
    "order_status": {
      "type": "enumeration",
      "enum": [
        "awaiting_confirmation",
        "confirmed",
        "shipped",
        "canceled"
      ]
    },
    "total_cost": {
      "type": "decimal"
    },
    "order_date": {
      "type": "date"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    }
  }
}
